{
  "name": "stagewise",
  "module": "src/index.ts",
  "type": "module",
  "version": "0.12.0-beta.4",
  "description": "Stagewise CLI",
  "author": "stagewise GmbH",
  "license": "AGPL-3.0-only",
  "repository": {
    "type": "git",
    "url": "https://github.com/stagewise-io/stagewise.git"
  },
  "private": false,
  "bin": {
    "stagewise": "dist/index.cjs"
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "dotenv -e ../../.env -- tsc --noEmit && dotenv -e ../../.env -- tsx build.ts",
    "dev": "NODE_ENV=development dotenv -e ../../.env -- tsx src/index.ts",
    "run": "dotenv -e ../../.env -- rsx dist/index.js",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:ui": "vitest run --ui",
    "test:coverage": "vitest run --coverage",
    "reset-data": "tsx reset.ts"
  },
  "devDependencies": {
    "@inquirer/prompts": "^7.0.0",
    "@stagewise/agent-prompt-snippets": "workspace:*",
    "@stagewise/agent-prompts": "workspace:*",
    "@stagewise/agent-knowledge": "workspace:*",
    "@stagewise/agent-runtime-interface": "workspace:*",
    "@stagewise/agent-runtime-node": "workspace:*",
    "@stagewise/agent-tools": "workspace:*",
    "@stagewise/agent-types": "workspace:*",
    "@stagewise/agent-utils": "workspace:*",
    "@stagewise/api-client": "1.4.0",
    "@stagewise/karton": "workspace:*",
    "@stagewise/karton-contract": "workspace:*",
    "@stagewise/toolbar": "workspace:*",
    "@types/express": "^4.17.21",
    "@types/prompts": "^2.4.9",
    "@types/ws": "^8.18.1",
    "@vitest/coverage-v8": "^2.0.0",
    "@vitest/ui": "^2.0.0",
    "axios": "^1.7.0",
    "chalk": "^5.3.0",
    "cli-table3": "^0.6.5",
    "commander": "^14.0.0",
    "dotenv-cli": "^9.0.0",
    "env-paths": "^3.0.0",
    "express": "^4.18.2",
    "http-proxy-middleware": "^3.0.5",
    "ignore": "^6.0.2",
    "license-checker": "^25.0.1",
    "open": "^10.0.0",
    "posthog-node": "^4.18.0",
    "typescript": "^5",
    "vitest": "^2.0.0",
    "winston": "^3.17.0",
    "zod": "4.0.17"
  },
  "dependencies": {
    "superjson": "2.2.2",
    "@ai-sdk/anthropic": "2.0.9",
    "@ai-sdk/google": "2.0.11",
    "@ai-sdk/openai": "^2.0.23",
    "@ai-sdk/provider": "^2.0.0",
    "@posthog/ai": "^6.4.3",
    "ai": "5.0.9",
    "diff": "^8.0.2",
    "esbuild": "0.25.9",
    "node-fetch": "^3.3.2",
    "prompts": "^2.4.2",
    "ws": "^8.18.3",
    "chokidar": "^4.0.3"
  }
}
