{
  "name": "@stagewise/agent-tools",
  "version": "0.4.0",
  "private": true,
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsx build.ts && tsc --emitDeclarationOnly",
    "dev": "tsx build.ts --watch",
    "clean": "rimraf dist",
    "lint": "echo 'No linting for tools-client package'",
    "type-check": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "3.0.7",
    "@ai-sdk/provider": "3.0.1",
    "@stagewise/api-client": "2.1.0",
    "ai": "6.0.11"
  },
  "devDependencies": {
    "@stagewise/agent-runtime-interface": "workspace:*",
    "@stagewise/agent-types": "workspace:*",
    "@vitest/coverage-v8": "^3.2.4",
    "esbuild": "0.25.9",
    "rimraf": "^5.0.0",
    "typescript": "^5.0.0",
    "vitest": "^3.2.4",
    "zod": "4.3.5"
  }
}
