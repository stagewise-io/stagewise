{
  "name": "@stagewise/agent-utils",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist/**/*.js",
    "dist/**/*.cjs",
    "dist/**/*.d.ts"
  ],
  "scripts": {
    "build": "pnpm run clean && pnpm run build:types && pnpm run build:bundle",
    "build:types": "tsc --emitDeclarationOnly",
    "build:bundle": "tsx build.ts",
    "dev": "tsx build.ts --watch",
    "clean": "rimraf dist",
    "lint": "echo 'No linting for agent utils package'",
    "type-check": "tsc --noEmit",
    "test": "vitest"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "2.0.9",
    "@ai-sdk/provider": "^2.0.0",
    "@stagewise/agent-knowledge": "workspace:*",
    "@stagewise/agent-runtime-interface": "workspace:*",
    "@stagewise/agent-tools": "workspace:*",
    "@stagewise/api-client": "1.4.0",
    "ai": "5.0.9",
    "xml": "^1.0.1",
    "zod": "4.0.17"
  },
  "devDependencies": {
    "@stagewise/agent-prompts": "workspace:*",
    "@stagewise/agent-types": "workspace:*",
    "@stagewise/karton-contract": "workspace:*",
    "esbuild": "0.25.9",
    "rimraf": "^5.0.0",
    "tsx": "^4.7.1",
    "typescript": "^5.0.0",
    "vitest": "^3.2.4"
  }
}
