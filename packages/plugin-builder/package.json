{
  "name": "@stagewise/plugin-builder",
  "description": "Build stagewise plugins with ease.",
  "version": "0.4.9",
  "private": false,
  "type": "module",
  "keywords": [
    "stagewise",
    "toolbar",
    "ai",
    "devtool",
    "agent",
    "interaction"
  ],
  "author": "tiq UG (haftungsbeschr√§nkt)",
  "license": "AGPL-3.0-only",
  "scripts": {
    "clean": "rm -rf .turbo node_modules dist",
    "dev": "tsc -b && node build.js",
    "build": "tsc -b && node build.js"
  },
  "homepage": "https://stagewise.io",
  "bugs": {
    "url": "https://github.com/stagewise-io/stagewise/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/stagewise-io/stagewise.git",
    "directory": "packages/toolbar"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.es.js",
      "default": "./dist/index.es.js"
    }
  },
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@stagewise/toolbar": "workspace:*",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "@types/react": "19.1.8",
    "vite": "6.3.5",
    "vite-plugin-dts": "4.5.3",
    "@vitejs/plugin-react-swc": "3.7.0",
    "rollup-preserve-directives": "1.0.0",
    "typescript": "^5.8.3"
  },
  "devDependencies": {
    "esbuild": "^0.25.5",
    "typescript": "^5.8.3"
  },
  "packageManager": "pnpm@10.10.0",
  "turbo": {
    "tasks": {
      "dev": {
        "outputs": [
          "dist/**"
        ]
      },
      "build": {
        "outputs": [
          "dist/**"
        ]
      }
    }
  }
}
