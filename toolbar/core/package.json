    "clsx": "^2.1.1",
    "lucide-react": "^0.503.0",
    "postcss": "^8.5.3",
    "postcss-prefix-selector": "^2.1.1",
    "preact": "^10.26.6",
    "tailwind-merge": "^3.2.0",
    "tailwindcss": "^4.1.5",
    "tsup": "^8.4.0",
    "typescript": "^5.8.3",
    "vite": "^6.3.5",
    "vite-bundle-analyzer": "^0.19.0",
